<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1" />
        <title>React Admin Starter</title>
        <% if (isProduction) { %>
        <script type="text/javascript">
            (function() {
                window.EXTERNAL__APP_CLIENT_API_URL__ = "$API_URL";
                window.EXTERNAL__APP_CLIENT_API_USER__ = "$API_USER";
                window.EXTERNAL__APP_CLIENT_API_PASSWORD__ = "$API_PASSWORD";
            })();
        </script>
        <% } %>
    </head>
    <body>
        <div id="page">
            <react-admin-starter />
        </div>
    </body>
</html>
